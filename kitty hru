end



con = mouse.Button1Down:Connect(function()
    if not c then
        con:Disconnect()
        return
    end


-----for the "for i=1, 10 do"
                                ---^    ^--this value executes the code how long it lasts
----the i=1 < this value delays the start of execution

---- if you want to add attacks just do --elseif attackCounter == value then-- to add more attacks at the last attack
    if not attack then  -- only execute attack if not on cooldown
        attackCounter = attackCounter + 1
        if attackCounter == 1 then --attack 1st
            attack = true
            for i=1, 10 do
                if event:Wait() then
                    punch()
                else
                    break
                end
            end
            for i=1, 10 do
                if event:Wait() then
                    punch2nd()
                else
                    break
                end
            end
            attack = false
            wait(2)  -- start cooldown
        elseif attackCounter == 2 then --attack 2nd
            attack = true
            for i=1, 10 do
                if event:Wait() then
                    kick()
                else
                    break
                end
            end
            for i=1, 10 do
                if event:Wait() then
                    kick2nd()
                else
                    break
                end
            end
            attack = false
            wait(2)  -- start cooldown
elseif attackCounter == 3 then --attack 3rd
            attack = true
            for i=1, 10 do
                if event:Wait() then
                    slap()
                else
                    break
                end
            end
            for i=1, 10 do
                if event:Wait() then
                    slap1()
                else
                    break
                end
            end
            attack = false
            attackCounter = 0
            wait(2)  -- start cooldown
        end
    end
if mode == "q" then
local t = mouse.Target
    if not t then
        return
    end
    attack = true
    for i=1, 5 do
        if event:Wait() then
            shootanim()
        else
            break
        end
    end
    spawn(function()
        if not t then
            return fling()
        end
        local p = t.Parent
        if p:IsA("Accessory") then
            p = p.Parent
        end
        if not p:IsA("Model") then
            return fling()
        end
        if not p:FindFirstChildOfClass("Humanoid") then
            return fling()
        end
        fling(p)
    end)
    for i=1, 5 do
        if event:Wait() then
            shootanim()
        else
            break
        end
    end
    attack = false
end    
end)

while event:Wait() do
if not attack then
    sine += 0.2
