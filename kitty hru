local function kick() --first frame of kick

RightShoulder.C0 = RightShoulder.C0:Lerp(cf(1, 0.5, 0) * angles(1.5707963267948966, 1.7453292519943295, -1.0471975511965976), 0.2)  LeftShoulder.C0 = LeftShoulder.C0:Lerp(cf(-1, 0.5, 0) * angles(1.5707963267948966, -1.9198621771937625, 2.0943951023931953), 0.2)  Neck.C0 = Neck.C0:Lerp(cf(0, 1, 0) * angles(-1.8325957145940461, 0, 3.141592653589793), 0.2)  RootJoint.C0 = RootJoint.C0:Lerp(cf(0, 0, -1) * angles(-1.2217304763960306, 0, 3.141592653589793), 0.2)  RightHip.C0 = RightHip.C0:Lerp(cf(1, -1, 0) * angles(0, 1.5707963267948966, -0.6981317007977318), 0.2)  LeftHip.C0 = LeftHip.C0:Lerp(cf(-1, -0.5, -0.8) * angles(0, -1.5707963267948966, -1.2217304763960306), 0.2)  --RightArm,1,0,0,1,90,0,0,1,0.5,0,0,1,100,0,0,1,0,0,0,1,-60,0,0,1,LeftArm,-1,0,0,1,90,0,0,1,0.5,0,0,1,-110,0,0,1,0,0,0,1,120,0,0,1,Head,0,0,0,1,-105,0,0,1,1,0,0,1,-0,0,0,1,0,0,0,1,180,0,0,1,Torso,0,0,0,1,-70,0,0,1,0,0,0,1,-0,0,0,1,-1,0,0,1,180,0,0,1,RightLeg,1,0,0,1,0,0,0,1,-1,0,0,1,90,0,0,1,0,0,0,1,-40,0,0,1,LeftLeg,-1,0,0,1,-0,0,0,1,-0.5,0,0,1,-90,0,0,1,-0.8,0,0,1,-70,0,0,1
end

local function kick2nd() --2nd frame of kick

